{
  "name": "peerjs-mesh",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "classnames": "^2.2.6",
    "peerjs": "^1.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src/tests"
    ],
    "testTimeout": 10000,
    "verbose": true,
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90
      }
    }
  },
  "devDependencies": {
    "lodash": "^4.17.21",
    "mock-socket": "^9.0.3",
    "uuid": "^8.3.2"
  }
}
